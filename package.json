{
  "name": "eslint-config-principled",
  "version": "1.1.1",
  "description": "A principle-based ESLint configuration for projects supporting TypeScript, React, and Jest",
  "scripts": {
    "commit": "cz",
    "lint": "npm run build && eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsc",
    "release": "standard-version"
  },
  "keywords": [
    "eslint",
    "config",
    "principled",
    "lint",
    "javascript",
    "typescript",
    "react",
    "jest"
  ],
  "bugs": "https://github.com/trevinhofmann/eslint-config-principled/issues",
  "homepage": "https://github.com/trevinhofmann/eslint-config-principled",
  "license": "Unlicense",
  "author": "Trevin Hofmann <trevinhofmann@gmail.com>",
  "main": "base.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/trevinhofmann/eslint-config-principled.git"
  },
  "files": [
    "README.md",
    "LICENSE",
    "CONTRIBUTING.md",
    "dist",
    "base.js",
    "typescript.js",
    "react.js",
    "jest.js"
  ],
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-typescript": "7.23.3",
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "@types/node": "20.11.10",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "babel-jest": "29.7.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "standard-version": "9.5.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
